{
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "proseWrap": "always",
  "plugins": ["prettier-plugin-organize-imports"],
  "importOrder": ["^@angular/(.*)$", "^rxjs/(.*)$", "^[./]"]
}